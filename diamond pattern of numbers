n=5
for i in range(n):
    #print space
    for j in range(n-i-1):
        print('',end=' ')
        #print numbers
    for k in range(i+1):
        print(n-k-1,end=" ")
    print()
for i in range(n-1):
    #print space
    for j in range(i+1):
        print('',end=' ')
        #print numbers
    for k in range(n-i-1):
        print(n-k-1,end=" ")
    print()
