# 1.This program shows various identities
str1 = "sandhya"
print(id(str1))

str2 = "sandhya"
print(id(str2))

# This will return True  
#if te string are not same then return false
print(id(str1) == id(str2))

# Use in Lists
list1 = ["aakash", "priya", "abdul"]
print(id(list1[0]))
print(id(list1[2]))

# This returns false
print(id(list1[0])==id(list1[2]))





# 2.working of type () function
a = ("Geeks", "for", "Geeks")
b = ["Geeks", "for", "Geeks"]
c = {"Geeks": 1, "for":2, "Geeks":3}
d = "Hello World"
e = 10.23
f = 11.22

print(type(a))
print(type(b))
print(type(c))
print(type(d))
print(type(e))
print(type(f))





#3. working of range functon

# print first 5 integers
# using python range() function
for i in range(5):
    print(i, end=" ")
print()

# printing a natural
# number from 5 to 20
for i in range(5, 20):
    print(i, end=" ")
print()

# Demonstration of Python range (start, stop, step)
for i in range(0, 10, 2):
    print(i, end=" ")
print()
